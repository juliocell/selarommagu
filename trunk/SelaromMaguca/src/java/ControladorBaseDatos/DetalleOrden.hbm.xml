<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ControladorBaseDatos" namespace="ControladorBaseDatos">

  <class name="DetalleOrden" table="DETALLEORDEN" lazy="false">

    <id name="Id">
		<column name="IdDetalleOrden" sql-type="number(10)" not-null="true" />
			<generator class="sequence">
				<param name ="sequence">
					Id_detalleorden
				</param>
			</generator>
    </id>

    <property name="CantidadProducto">
		<column name="CantidadProducto" sql-type="number(10)" not-null="true"/>
	</property>

    <many-to-one name="Producto" column="fk_IdProducto" class="Producto"/>


  </class>
</hibernate-mapping>