    <faces-config xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
version="2.0">
    
<managed-bean>
    <managed-bean-name>controladorCargo</managed-bean-name>
    <managed-bean-class>controlador.ControladorCargo</managed-bean-class>

    <managed-bean-scope>session</managed-bean-scope></managed-bean>

<managed-bean>
    <managed-bean-name>cargo</managed-bean-name>
    <managed-bean-class>datos.Cargo</managed-bean-class>

    <managed-bean-scope>session</managed-bean-scope></managed-bean>

<managed-bean>
    <managed-bean-name>loginController</managed-bean-name>
    <managed-bean-class>controlador.LoginController</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>


<navigation-rule>
    <from-view-id>/index.xhtml</from-view-id>
<navigation-case>
<from-outcome>cargoPrincipal</from-outcome>
<to-view-id>/cargoPrincipal.xhtml</to-view-id>
</navigation-case>
</navigation-rule>



</faces-config>




